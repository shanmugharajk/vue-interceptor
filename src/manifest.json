{
  "manifest_version": 2,
  "name": "Interceptor",
  "description": "An extension to help you redirect urls, modify headers.",
  "default_locale": "en",
  "permissions": ["<all_urls>", "*://*/*"],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Interceptor",
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    }
  },
  "options_ui": {
    "page": "options.html",
    "browser_style": true
  }
}
