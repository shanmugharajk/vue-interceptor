{
  "manifest_version": 2,
  "name": "Interceptor",
  "description": "An extension to help you redirect urls, modify headers.",
  "default_locale": "en",
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "tabs"
  ],
  "icons": {
    "16": "icons/icon-16x16.png",
    "48": "icons/icon-48x48.png",
    "128": "icons/icon-128x128.png"
  },
  "browser_action": {},
  "options_page": "options.html",
  "background": {
    "scripts": ["js/background.js"]
  }
}
